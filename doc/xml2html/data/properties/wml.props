DL			= org.runtime.manuals.html.DLElement

BIBLIOGRAPHY		= org.runtime.manuals.wml.BibliographyElement
BIBITEM			= org.runtime.manuals.html.BibItemElement
CITE			= org.runtime.manuals.html.CiteElement

URL			= org.runtime.manuals.html.URLElement
EMAIL			= org.runtime.manuals.html.EmailElement

TABLE			= org.runtime.manuals.html.TableElement
