. ./common.sh
flavor="test_marcel_cleanup"
prog="cleanup"
sort_output=1

(
    echo "Bye bye all!"
    echo "Bye bye boys!"
    echo "Bye bye everybody!"
    for i in $(seq 1 10) ; do
	echo "Hi all!"
    done
    for i in $(seq 1 3) ; do
	echo "Hi boys!"
    done
    for i in $(seq 1 5) ; do
	echo "Hi everybody!"
    done
    for i in $(seq 1 10) ; do
	echo "Hi girls!"
    done
) > /tmp/pm2test_"${USER}"_expected
