do_clear() {
    do_clear_modules
    do_clear_all
}

do_clear_modules() {
    modules=
}

do_clear_all() {
    for mod in $MODULES; do
	do_clear_mod $mod
    done
}

do_clear_mod() { # module
    eval lst_opt_${1}=""
}

do_add() {
    addsub=add
}

do_sub() {
    addsub=sub
}

do_list_addsub() { # module liste_opt
    for opt in $2 " "; do
	[ " " = "$opt" ] && continue
	verify_option $1 $opt
	eval do_list_$addsub $1 $opt
    done
}

do_list_add() { # module opt
    lst_opt="`eval echo \\$lst_opt_$1`"
    case " $lst_opt " in
    *\ $2\ *)
	;;
    *)
	eval lst_opt_$1='"${lst_opt:+$lst_opt }$2"'
	;;
    esac
}

do_list_sub() { # module liste_opt
    lst_opt=
    for opt in `eval echo \\$lst_opt_$1` " "; do
	case $opt in
	" "|$2)
	    ;;
	*)
	    lst_opt="${lst_opt:+$lst_opt }$opt"
	    ;;
	esac
    done
    eval lst_opt_$1='"${lst_opt}"'
}

##########################################################################
# Check functions

vide=

do_read_provide() {
    $ECHO "Reading provides..."
    for mod in $HIDDEN_MODULES $modules; do
	for name in `"$PM2_MODULE" module=$mod --provide`; do
	    before="`eval echo \\$provide_${name}`"
	    eval provide_${name}='"${before:+$before }$mod"'
	    $DEBUG providing ${name} by $mod
	done
	lst_opt="`eval echo \\$lst_opt_${mod}`"
        [ -z "$lst_opt" ] && continue
	for opt in $lst_opt; do
            opt=${opt%%:*}
	    for name in `"$PM2_MODULE" option=$opt --module=$mod --provide`; do
                before="`eval echo \\$provide_${name}`"
                eval provide_${name}='"${before:+$before }${mod}_$opt"'
		$DEBUG providing ${name} by ${mod}_$opt
	    done
	done
    done
}

do_check_depend() {
    $ECHO "Checking depends..."
    depend_list=""
    for mod in $HIDDEN_MODULES $modules; do
	for name in `"$PM2_MODULE" module=$mod --depend`; do
	    value="`eval echo \\$provide_${name}`"
	    if [ -z "$value" ]; then
		check_result=1
		depend_list="${depend_list:+$depend_list }$name"
		echo "Depend \"$name\" from module '$mod' unsatisfied"
	    fi
	done
	lst_opt="`eval echo \\$lst_opt_${mod}`"
	[ -z "$lst_opt" ] && continue
	for opt in $lst_opt; do
	    for name in `"$PM2_MODULE" option=$opt --module=$mod --depend`; do
		value="`eval echo \\$provide_${name}`"
		if [ -z "$value" ]; then
		    check_result=1
		    echo "Depend \"$name\" from option '$mod/$opt' unsatisfied"
		    depend_list="${depend_list:+$depend_list }$name"
		fi
	    done
	done
    done
    if [ -z "$depend_list" -o "$check_no_resolution" = true ]; then
	return 0
    fi
    for mod in $MODULES; do
	for name in `"$PM2_MODULE" module=$mod --provide`; do
	    case " $depend_list " in
	    *\ $name\ *)
		echo "Module '$mod' provides \"$name\""
		;;
	    *)
		;;
	    esac
	done
	lst_opt=`"$PM2_MODULE" options --module=$mod`
	[ -z "$lst_opt" ] && continue
	for opt in $lst_opt; do
	    for name in `"$PM2_MODULE" option=$opt --module=$mod --provide`; do
		case " $depend_list " in
		*\ $name\ *)
		    echo "Option '$opt' from modules '$mod' provides \"$name\""
		    ;;
		*)
		    ;;
		esac
	    done
	done
    done
}

do_check_confict() {
    $ECHO "Checking conflicts..."
    for mod in $HIDDEN_MODULES $modules; do
	for name in `"$PM2_MODULE" module=$mod --conflict`; do
	    value="`eval echo \\$provide_${name}`"
	    if [ \( ! -z "$value" \) -a \( "$value" != "$mod" \) ]; then
		echo "$name in module $mod conflicts with `echo $value' '\
		    | sed -e s,$mod' ',,`"
		check_result=1
	    fi
	done
	lst_opt="`eval echo \\$lst_opt_${mod}`"
	[ -z "$lst_opt" -o "$check_no_resolution" = true ] && continue
	for opt in $lst_opt; do
	    for name in `"$PM2_MODULE" option=$opt --module=$mod --conflict`; do
		value="`eval echo \\$provide_${name}`"
		if [ \( ! -z "$value" \) -a \( "$value" != "${mod}_$opt" \) ]; then
		    echo "$name in option $mod/$opt conflicts with `  \
			echo $value' '  | sed -e s,$mod/$opt' ',,`"
		    check_result=1
		fi
	    done
	done
    done
}

##########################################################################
# Main fonctions
do_version() {
    echo pm2-libs 3.0
    exit 0
}

do_set() {
    do_add
    while test $# -gt 0; do
	case "$1" in
	--flavor=*)
	    set_option "$1" 1 '-_[a-z][A-Z][0-9]' ""
	    ;;
	--builddir=*|--ext=*)
	    set_option "$1" 1 '-/\.\$_[a-z][A-Z][0-9]' "ok"
	    ;;	
	--clear|--clear-modules|--clear-all|--add|--sub)
	    optname=${1#--}
	    optname=${optname//-/_}
	    eval do_$optname
	    ;;
	--clear-*)
	    optname=${1#--clear-}
	    case " $MODULES " in
	    *\ $optname\ *)
		do_clear_mod $optname
		;;
	    *)
		echo "Ignoring unknown module \`$optname' in clear option" 1>&2
		exit 1
		;;
	    esac
	    ;;
	--all=*)
	    optarg="${1#*=}"
	    for mod in $MODULES; do
		do_list_addsub $mod "$optarg"
	    done
	    ;;
	--*=*)
	    optname="${1#--}"
	    optname="${optname%%=*}"
	    optarg="${1#*=}"
	    if [ -n "${optname//[-_a-zA-Z0-9]}" ]; then
		echo Unknown option \`$1\' 1>&2
		usage 1
	    fi
	    case " $MODULES modules " in
	    *\ $optname\ *)
		do_list_addsub $optname "$optarg"
		;;
	    *)
		echo "Ignoring unknown module \`$optname' options" 1>&2
		exit 1
		;;
	    esac
	    ;;
	*)
	    echo Unknown option \`$1\' 1>&2
	    usage 1
	    ;;
	esac
	shift
    done
    if [ -z "$flavor" ]; then
	flavor="${PM2_FLAVOR:-@PM2_DEFAULT_FLAVOR@}"
    fi
    if [ -z "$builddir" ]; then
	builddir='$flavor'
    fi

    if [ -f "`find_flavor`" ]; then
	msg="Modifying flavor '$flavor'"
    else
	msg="Creating flavor $flavor"
    fi
    create_flavor "$msg"
}

do_delete() {
    file=
    while test $# -gt 0; do
	case "$1" in
	--flavor=*)
	    set_option "$1" 1 '-_[a-z][A-Z][0-9]' ""
	    file="`find_flavor`"
	    ;;	
	*)
	    echo Unknown option \`$1\' 1>&2
	    usage 1
	    ;;
	esac
	shift
    done
    if [ -f "$file" ]; then
	delete_flavor
    else
	echo "Flavor $flavor does not exist. Cannot delete it"
    fi
    exit 0
}

do_list() {
    test $# -gt 0 && usage 1 list
    if get_liste_flavor; then
	echo "$liste_flavor"
	exit 0
    fi
    exit 1
}

do_get() {
    file=
    while test $# -gt 0; do
	case "$1" in
	--flavor=*)
	    set_option "$1" 1 '-_[a-z][A-Z][0-9]' ""
	    file="`find_flavor`"
	    nb_fromfile=$nb_flavor
	    ;;	
	--fromfile=*)
	    set_option "$1" 1 "" ""
	    file="$fromfile"
	    nb_flavor=$nb_fromfile
	    ;;	
	*)
	    echo Unknown option \`$1\' 1>&2
	    usage 1
	    ;;
	esac
	shift
    done
    if [ -z "$file" ]; then
	flavor="${PM2_FLAVOR:-@PM2_DEFAULT_FLAVOR@}"
	file="`find_flavor`"
    fi
    if [ ! -f "$file" ]; then
	if [ -z "$flavor" ]; then
	    echo "$file does not exist" 1>&2
	else
	    echo "flavor '$flavor' does not exist" 1>&2
	fi
	exit 1
    fi
    cat "$file" | grep '^'"$SETTINGS" | sed -e "s,^$SETTINGS,,"
    exit 0
}

do_get_ac() {
    file=
    while test $# -gt 0; do
	case "$1" in
	--flavor=*)
	    set_option "$1" 1 '-_[a-z][A-Z][0-9]' ""
	    file="`find_flavor`"
	    nb_fromfile=$nb_flavor
	    ;;	
	--fromfile=*)
	    set_option "$1" 1 "" ""
	    file="$fromfile"
	    nb_flavor=$nb_fromfile
	    ;;	
	*)
	    echo Unknown option \`$1\' 1>&2
	    usage 1
	    ;;
	esac
	shift
    done
    if [ -z "$file" ]; then
	flavor="${PM2_FLAVOR:-libpthread}"
	file="`find_flavor`"
    fi
    if [ ! -f "$file" ]; then
	if [ -z "$flavor" ]; then
	    echo "$file does not exist" 1>&2
	else
	    echo "flavor '$flavor' does not exist" 1>&2
	fi
	exit 1
    fi
    cat "$file" | grep '^'"$SETTINGS" | sed -e "s,^$SETTINGS,," | \
    	grep -v -- --flavor= | \
	grep -v -- --builddir= | \
	grep -v -- --ext= | \
	sed -e 's/--modules=\([^ ]*\)/--enable-\1/g' | \
	sed -e 's/--\([^= ]*\)=\([^ ]*\)/--enable-\1-\2/g' | \
	sed -e 's/:/=/g' | \
	sed -e 's/_/-/g'
    if ! cat "$file" | grep '^'"$SETTINGS" | grep '=opt' > /dev/null
    then
	# No opt, disable opt
	echo --disable-opt
    fi
    if ! cat "$file" | grep '^'"$SETTINGS" | grep 'build_static' > /dev/null
    then
	# No static, disable static
	echo --disable-static
    fi
    exit 0
}

do_help() {
    usage 0 "$1"
    exit 0
}

add_option() { # Option(--"module"=opt) liste_to_ignore
	optname="${1#--}"
	optname="${optname%%=*}"
	optarg="${1#*=}"
	if [ -n "${optname//[-_a-zA-Z0-9]}" ]; then
	    echo Unknown option \`$1\' 1>&2
	    usage 1
	fi
	case " $MODULES modules " in
	    *\ $optname\ *)
	    do_list_addsub $optname "$optarg"
	    ;;
	*)
	    case \ "$2"\  in
	        *\ "$optname"\ *)
	            ;;
	    	*)
	    	    echo "Ignoring unknown module \`$optname' options" 1>&2
	    	    exit 1
		    ;;
	    esac
	    ;;
	esac
}

do_check() {
    do_add
    while test $# -gt 0; do
	case "$1" in
	--no-resolution)
	    check_no_resolution=true
	    ;;
	--flavor=*)
	    set_option "$1" 1 '-_[a-z][A-Z][0-9]' ""
	    options=`do_get "$1"`
	    if test $? -eq 0; then
		for opt in $options; do
	    	    add_option "$opt" "flavor builddir ext"
		done
	    else
		exit 1
	    fi
	    ;;
	--builddir=*|--ext=*)
	    #set_option "$1" 1 '-_[a-z][A-Z][0-9]' "ok"
	    ;;	
	--clear|--clear-modules|--clear-all|--add|--sub)
	    optname=${1#--}
	    optname=${optname//-/_}
	    eval do_$optname
	    ;;
	--clear-*)
	    optname=${1#--clear-}
	    case " $MODULES " in
	    *\ $optname\ *)
		do_clear_mod $optname
		;;
	    *)
		echo "Ignoring unknown module \`$optname' in clear option" 1>&2
		exit 1
		;;
	    esac
	    ;;
	--all=*)
	    optarg="${1#*=}"
	    for mod in $MODULES; do
		do_list_addsub $mod "$optarg"
	    done
	    ;;
	--*=*)
	    add_option "$1" ""
	    ;;
	*)
	    echo Unknown option \`$1\' 1>&2
	    usage 1
	    ;;
	esac
	shift
    done
    if [ -z "$flavor" ]; then
	flavor="${PM2_FLAVOR:-@PM2_DEFAULT_FLAVOR@}"
    fi
    modules=
    for mod in $lst_opt_modules " "; do
	case $mod in
	" "|$HIDDEN_MODULES_OR)
	    ;;
	*)
	    modules="${modules:+$modules }$mod"
	    ;;
	esac
    done
    check_result=0
    do_read_provide
    do_check_depend
    do_check_confict
    exit $check_result
}

do_regenerate() {
    file=
    while test $# -gt 0; do
	case "$1" in
	--flavor=*)
	    set_option "$1" 1 '-_[a-z][A-Z][0-9]' ""
	    file="`find_flavor`"
	    nb_fromfile=$nb_flavor
	    ;;
	*)
	    echo Unknown option \`$1\' 1>&2
	    usage 1
	    ;;
	esac
	shift
    done
    if [ ${EDIT_MODE} = "normal" ]; then
	flag=""
    else
	flag="--admin"
    fi
    for f in ${flavor:-`"$PM2_FLAVOR_PROG" ${flag} list`} ; do
	"$PM2_FLAVOR_PROG" ${flag} set \
		`"$PM2_FLAVOR_PROG" ${flag} get --flavor=$f`
    done

    exit 0
}

do_find() {
    file=
    while test $# -gt 0; do
	case "$1" in
	--flavor=*)
	    set_option "$1" 1 '-_[a-z][A-Z][0-9]' ""
	    ;;
	*)
	    echo Unknown option \`$1\' 1>&2
	    usage 1
	    ;;
	esac
	shift
    done

    flavor=${flavor:-default}
    echo `find_flavor`
}

# Programme principal

if test $# -eq 0; then
    usage 1
fi

DEBUG=":"
ECHO=":"
EDIT_MODE="normal"
while test $# -gt 0; do
    CMD="$1"
    case "$CMD" in
    set)
	shift
	$DEBUG "Executing $CMD"
	eval do_$CMD '${@:+"$@"}'
	do_check ${@:+"$@"}
	exit 0
	;;
    version|set|delete|list|get|get_ac|help|check|regenerate|find)
	shift
	$DEBUG "Executing $CMD"
	eval do_$CMD '${@:+"$@"}'
	exit $?
	;;
    --verbose|-v)
	if [ "$ECHO" = ":" ]; then
	    ECHO=echo
	else
	    DEBUG=echo
	fi
	;;
    --admin|-a)
	echo "*admin* flag is obsolete; do not use any more."
	exit
        # EDIT_MODE="admin"
	;;
    -h|--help)
	do_help
	exit 0
	;;
    *)
	usage 1 "$CMD"
	;;
    esac
    shift
done

exit 0
