define __set-ctx1
  set $ebp=((marcel_t)$arg0)->ctx_yield[0].jbuf[3]
end

define __set-ctx2
  set $esp=((marcel_t)$arg0)->ctx_yield[0].jbuf[4]
end

define __set-ctx3
  set $eip=((marcel_t)$arg0)->ctx_yield[0].jbuf[5]
end

define __set-ctx
  __set-ctx1 $arg0
  __set-ctx2 $arg0
  __set-ctx3 $arg0
end
