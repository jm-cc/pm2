sub MoveSW {
    my $self=shift;
    my $utid=$self->LastSW;
    my $ev;
    
    if (defined($self->DelayedUEV)) {
	foreach $ev (@{$self->DelayedUEV}) {
	    $debug && print "moving $ev to $utid: ";
	    my $field;
	    for $field (@{$ev}) {
	        $debug && print $field, " ";
	    }
	    $debug && print "\n";
	    SuperTrace::Update_Utid($ev, $utid);
	}
    }
    $self->DelayedUEV(0);
}

