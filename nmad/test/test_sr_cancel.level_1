# Configuration
create_test_hosts 2;
net=${TESTNET:-tcp}
mpi=""
strat="strat_default"
net2=$(echo $net | tr ' ' '_')
flavor="test_nmad_${mpi}_${net2}"

appdir="${PM2_SRCROOT}/nmad/examples/sendrecv"
prog="sr_cancel"
args="--x --p --l"

rm -f /tmp/pm2test_"${USER}"_expected
(cat <<EOF
buffer contents: hello, world
nm_sr_cancel rc = -10 (expected: -NM_EALREADY = -10)
nm_sr_cancel rc =   0 (expected:  NM_ESUCCESS = 0)
nm_sr_test   rc = -14 (expected: -NM_ECANCELED = -14)
nm_sr_cancel rc =  -2 (expected: -NM_ENOTIMPL = -2)
nm_sr_cancel rc =   0 (expected:  NM_ESUCCESS = 0)
nm_sr_cancel rc =   0 (expected:  NM_ESUCCESS = 0)
EOF
) > /tmp/pm2test_"${USER}"_expected

. ./include/nmad_default

