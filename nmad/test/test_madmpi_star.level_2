# Configuration
hosts=${TESTCONFIG:-"localhost localhost"}
net=${TESTNET:-tcp}
mpi="mpi_emu"
strat="strat_default"
net2=$(echo $net | tr ' ' '_')
flavor="test_nmad_${mpi}_${net2}"
sort_output=1

appdir="${PM2_ROOT}/nmad/examples/mpi/basics"
prog="star"
args="--x --p --l"

rm -f /tmp/pm2test_"${USER}"_expected
nb_hosts=$(echo $hosts | wc -w)
ranks=$(expr $nb_hosts - 1)
(
for n in $(seq 1 $ranks) ; do
    echo "ISending to child $n completed"
done
for n in $(seq 1 $ranks) ; do
    echo "Message from child $n [12.450000,3.140000]"
done
for n in $(seq 1 $ranks) ; do
    echo "Waiting for sending to child $n completed"
done
) > /tmp/pm2test_"${USER}"_expected

. ./include/nmad_default

