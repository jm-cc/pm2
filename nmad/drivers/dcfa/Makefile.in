NewMad_dcfa_rcache: MODLIBS += --requires NewMad_dcfa_common
NewMad_dcfa_rcache: nm_dcfa_rcache.pic.o

NewMad_dcfa_lr2: MODLIBS += --requires NewMad_dcfa_common
NewMad_dcfa_lr2: nm_dcfa_lr2.pic.o
