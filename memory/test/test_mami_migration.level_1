. ./mami.sh
prog="migration"
sort_output=1
nodes=$(ls -d /sys/devices/system/node/node* | sed 's:/sys/devices/system/node/node::g')
for cnode in $(echo $nodes) ; do
    for mnode in $(echo $nodes) ; do
        if [ "$cnode" != "$mnode" ] ; then
            echo "[$cnode:$mnode] Data has succesfully been migrated"
        fi
    done
done > /tmp/pm2test_"${USER}"_expected
