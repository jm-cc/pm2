#!/bin/sh

dir=$(dirname $0)
arch=$(pm2-arch --pm2-asm)
link=$(readlink $0)
if [ -n "$link" ] ; then
    dir=$(echo $dir"/"$(dirname $link))
fi
if [ ! -f $dir/ext/obj/$arch/arg_ifort.o ] ; then
    mkdir -p $dir/ext/obj/$arch
    $CC $USER_CFLAGS $dir/ext/arg_ifort.c -c -o $dir/ext/obj/$arch/arg_ifort.o
fi
