    if (scalar @_ == 1) {
	my $value=shift;
	if (defined($value)) {
	    $self->{__PACKAGE__ . ".info"}->{$name}->{$key}=$value;
	} else {
	    if (defined($self->{__PACKAGE__ . ".info"}->{$name})
		&& defined($self->{__PACKAGE__ . ".info"}->{$name}->{$key})) {
		    delete($self->{__PACKAGE__ . ".info"}->{$name}->{$key});
		}
	}
	return $value;
    }

    if (defined($self->{__PACKAGE__ . ".info"}->{$name})
	&& defined($self->{__PACKAGE__ . ".info"}->{$name}->{$key})) {
	return $self->{__PACKAGE__ . ".info"}->{$name}->{$key};
    }

    return undef;    
}

