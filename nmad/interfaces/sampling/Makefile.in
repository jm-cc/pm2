# makefile for sampling

srcdir := @srcdir@
VPATH  := $(srcdir)

include @abs_top_builddir@/common_vars.mk

TARGET_BIN = sampling-prog
TARGET_LIB =
SUBDIRS =

INSTALL_BIN     = $(TARGET_BIN) nmad-sample
INSTALL_LIB     = $(TARGET_LIB)
