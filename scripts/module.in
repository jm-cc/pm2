#%Module
#
# module generated by pm2-build-packages

proc ModulesHelp { } {

        puts stderr "\tAdd pm2 packages to your environment"
}

module-whatis    "Add pm2 packages to your environment"

set              prefix           @prefix@

prepend-path     PATH             $prefix/bin
prepend-path     LD_LIBRARY_PATH  $prefix/lib
prepend-path     PKG_CONFIG_PATH  $prefix/lib/pkgconfig
prepend-path     CPATH            $prefix/include

