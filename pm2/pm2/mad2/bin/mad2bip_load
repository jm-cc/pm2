#!/bin/sh

chaine=""
for i in `cat ${PM2_CONF_FILE}`; do
    chaine="${chaine} `basename $i -t`"
done

bipconf $chaine

dbgopt=""
if [ ${PM2_DEBUG} = "on" ]; then
    confsize=`cat ${PM2_CONF_FILE} | wc -w`
    n=0
    while [ $n -le $confsize ]; do
	dbgopt="$dbgopt -d $n"
	n=`expr $n + 1`
    done
fi

bipload $dbgopt -mget -hwflow "$@"

