    my $id=$self->readId($ev);
    if (defined($id)) {
	my $entity=$self->getId($id);
	if (not defined($entity)) {
	    @ret=$self->create($id, $ev);
	    $entity=$self->getId($id);
	}
	$self->getCollection->setEventEntity($ev, $entity);
    }

    my $ret=shift @ret;
    for $ev (@ret) {
	$self->addPreEv($ev);
    }
    return $ret;
}

