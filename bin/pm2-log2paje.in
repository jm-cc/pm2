	$info=shift;
    }
}

sub insert {
    $_=shift;
    chomp;
    $debug && print "--------------------------------------------------\n";
    $debug && print "Insert: ",$_,"\n";
    my @l=split;
    if ($l[0] eq "K") {
	FxT::InsertFKT($l[1], $l[2], $l[3], \@l);
    } else {
	FxT::InsertFUT($l[1], $l[3], \@l);
    }
}

