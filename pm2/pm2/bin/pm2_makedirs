if [ "`echo -n`" != "-n" ] ; then
	ECHO='echo -n .'
elif [ -x /usr/bin/printf ] ; then
	ECHO='printf .'
else
	ECHO='echo .'
fi

$ECHO
if [ ! -d AIX/RS6K ]; then
    mkdir -p AIX/RS6K
fi
$ECHO
if [ ! -d FREEBSD/X86 ]; then
    mkdir -p FREEBSD/X86
fi
$ECHO
if [ ! -d IRIX/MIPS ]; then
    mkdir -p IRIX/MIPS
fi
$ECHO
if [ ! -d LINUX/ALPHA ]; then
    mkdir -p LINUX/ALPHA
fi
$ECHO
if [ ! -d LINUX/X86 ]; then
    mkdir -p LINUX/X86
fi
$ECHO
if [ ! -d LINUX/PPC ]; then
    mkdir -p LINUX/PPX
fi
$ECHO
if [ ! -d OSF/ALPHA ]; then
    mkdir -p OSF/ALPHA
fi
$ECHO
if [ ! -d SOLARIS/SPARC ]; then
    mkdir -p SOLARIS/SPARC
fi
$ECHO
if [ ! -d SOLARIS/X86 ]; then
    mkdir -p SOLARIS/X86
fi
$ECHO
if [ ! -d UNICOS/ALPHA ]; then
    mkdir -p UNICOS/ALPHA
fi

