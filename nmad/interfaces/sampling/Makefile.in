TARGETS = subdirs $(TARGET_BIN) $(TARGET_LIB)

CFLAGS  := @NMAD_CORE_CFLAGS@
LDFLAGS := @NMAD_CORE_LDFLAGS@
BINLIBS := -L@nmad_builddir@ @NMAD_LIBS@ @NMAD_CORE_LIBS@

include @abs_top_builddir@/common_rules.mk

$(TARGET_BIN): %: %.pic.o
