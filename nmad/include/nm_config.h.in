/** @file public config for nmad.
 * This file contains only prefixed symbols and may be safely installed
 */

/* use 8 bits tags in arrays */
#undef NM_TAGS_AS_FLAT_ARRAY

/* use 32 bits tags in hashtable */
#undef NM_TAGS_AS_HASHTABLE

/* use 64 bits tags in hashtable */
#undef NM_TAGS_AS_INDIRECT_HASH

/* enable Mad-MPI */
#undef MAD_MPI

/* GNU gfortran Fortran support */
#undef NMAD_FORTRAN_TARGET_GFORTRAN

/* Intel ifort Fortran support */
#undef NMAD_FORTRAN_TARGET_IFORT

/* no Fortran support */
#undef NMAD_FORTRAN_TARGET_NONE

/* enable sampling */
#undef NMAD_SAMPLING

/* nmad install path */
#undef NMAD_ROOT

/* include ROMIO in mpi.h */
#undef NMAD_MPI_ENABLE_ROMIO

/* build nmad with pioman */
#undef NMAD_PIOMAN

#undef NMAD_PUKABI
