    my $self=shift;
    my ($date)=undef;
    my ($source)=undef;
    my $s;
    for $s (@{$self->sources()}) {
	my $ev=$s->{"colEv"}->nextEv();
	if (defined($ev)) {
	    my $d=Event::date($ev);
	    if ((not defined($date)) or ($d < $date)) {
		$date=$d;
		$source=$s;
	    }
	}
    }
    if (defined($source)) {
	return $source->{"colEv"}->getEv();
    }
    return undef;
}

